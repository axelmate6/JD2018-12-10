<?xml version="1.0" encoding="UTF-8" ?>
<schema xmlns="http://www.w3.org/2001/XMLSchema"
        targetNamespace="http://beans.jd02_09.kushnerov.by.it/"
        xmlns:tag="http://beans.jd02_09.kushnerov.by.it/"
        elementFormDefault="qualified"
>
<element name="users" >
    <complexType>
        <sequence>
            <element
                    name="user"
                    type="tag:User"
                    minOccurs="2"
                    maxOccurs="unbounded" />
        </sequence>
    </complexType>
</element>

<complexType name="User">
    <sequence>
        <element name="login" type="ID" />
        <element name="email" type="string" />
        <element name="password" type="string" />
        <element name="ads"  type="tag:Ads"/>
    </sequence>
</complexType>

<complexType name="Ads">
    <sequence>
        <element name="ad"
                 minOccurs="0"
                 maxOccurs="unbounded"
                 type="tag:Ad"/>
    </sequence>
</complexType>

<complexType name="Ad">
    <sequence>
        <element name="description" type="string"/>
        <element name="address" type="string"/>
        <element name="floor" type="integer"/>
        <element name="floors" type="positiveInteger"/>
        <element name="rooms" type="positiveInteger"/>
        <element name="price" type="double"/>
        <element name="area" type="double"/>
    </sequence>
</complexType>


</schema>